<div class='terms-temp container-custom py-24'>
  {% section 'legal-hero' %}
  <main class='w-full sm:max-w-[75vw] xl:max-w-[50vw] mx-[auto]'>
    <div class='blog-content py-[36px] w-full font-inter text-base'>
      {{ page.content }}
    </div>
    <p class='mt-[2rem]'>
      Last Updated:
      {{- page.published_at | date: '%B %d, %Y' -}}
    </p>
  </main>
</div>
